                    $("#SubmittedCanceledDocumentsCount").html(data.SubmittedCanceledDocumentsCount);
                    $("#SubmittedRejectedDocumentsCount").html(data.SubmittedRejectedDocumentsCount);
                    $("#SubmittedSubmittedDocumentsCount").html(data.SubmittedSubmittedDocumentsCount);
                    KTApp.unblock('#Card7');
                    KTApp.unblock('#Card8');
                    KTApp.unblock('#Card9');
                },
                error: function (xhr) {
                    //Do Something to handle error
                    Swal.fire({
                        title: "@MasterResources.OoopsLbl",
                        text: "@MasterResources.ConnectionErrorMsg",
                        icon: "error",
                        buttonsStyling: false,
                        confirmButtonText: "@MasterResources.TryAgainBtn",
                        showCancelButton: true,
                        cancelButtonText: "@MasterResources.SettingBtn",
                        customClass: {
                            confirmButton: "btn btn-danger",
                            cancelButton: "btn btn-default"
                        }
                    }).then(function (result) {
                        if (result.value) {
                            window.location.href = "/ETA.Hub/einvoicing/v0";
                            KTUtil.scrollTop();
                        } else if (result.dismiss === "cancel") {
                            window.location.href = "/v1/appsetting";
                        }
                    });
                }
            });
        });
    });
</script>

