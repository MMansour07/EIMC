        function HandlingSideNavActivity() {

            var lastsegement = window.location.pathname.split('?')[0].split('/').filter(function (i) { return i !== "" }).slice(-1)[0] ?? 'v1';
            var _currentPage = lastsegement.toLocaleLowerCase();

            if (_currentPage == 'pending' || _currentPage == 'submitted' ||
                _currentPage == 'received' || _currentPage == 'invalid_failed')
                $('.doc-menu').addClass('menu-item-open');

            if (_currentPage == 'new_document') {
                $('.doc-menu').addClass('menu-item-open');
                $('.initiate-menu').addClass('menu-item-open');
            }

            $('.' + _currentPage).addClass('menu-item-active');
