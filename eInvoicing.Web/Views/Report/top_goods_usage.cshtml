
@model IEnumerable<eInvoicing.DTO.DocumentDTO>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<title>EInvoicing|Documents</title>
<div class=" container ">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6  subheader-transparent " id="kt_subheader">
        <div class=" container  d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">

                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold my-1 mr-5">
                        Top Goods Usage
                    </h5>
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">
                                Reports
                            </a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">
                                Stats
                            </a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
        </div>
    </div>
    <!--end::Subheader-->
    <div class="form-group row mb-4">
        <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="input-group date">
                <input type="text" class="form-control" id="fromDate" readonly="readonly" placeholder="From Date">
                <div class="input-group-append">
                    <span class="input-group-text">
                        <i class="la la-calendar"></i>
                    </span>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6 col-sm-12">
            <div class="input-group date">
                <input type="text" class="form-control" id="toDate" readonly="readonly" placeholder="To Date">
                <div class="input-group-append">
                    <span class="input-group-text">
                        <i class="la la-calendar"></i>
                    </span>
                </div>
            </div>
        </div>
        <a href="#" id="_find" class="btn btn-light-primary btn-shadow">
            <i class="flaticon2-search"></i>
            Find
        </a>
    </div>
    <hr />
    <div class="card card-custom  gutter-b" id="SubmittedDocumentsStats_crd">
        <div class="card-body">
            <!--begin: Datatable-->
            <table class="table table-hover table-checkable" id="kt_datatable10">
                <thead>
                    <tr>
                        <th>Code</th>
                        <th>Description</th>
                        <th>Count</th>
                        <th>Amount</th>
                        <th>Tax</th>
                    </tr>
                </thead>
            </table>
            <!--end: Datatable-->
        </div>
    </div>
</div>
<script src="~/Content/assets/plugins/custom/datatables/datatables.bundle.js?v=7.0.6"></script>
<script src="~/Content/assets/js/pages/crud/datatables/search-options/TopGoods.js?v=7.0.6"></script>
<script src="~/Content/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js?v=7.0.6"></script>
<script src="~/Content/assets/js/pages/crud/forms/widgets/bootstrap-datepicker.js"></script>