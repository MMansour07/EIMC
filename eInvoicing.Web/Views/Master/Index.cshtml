@using eInvoicing.Web.Views.Master
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string LanguageImgSrc;
    if (Request.Cookies["Language"].Value == "en")
    {
        LanguageImgSrc = "/Content/images/226-united-states.svg";
    }
    else
    {
        LanguageImgSrc = "/Content/images/158-egypt.svg";
    }
}

<div class="container-fluid">
    <div class="subheader py-2 py-lg-6  subheader-transparent" id="kt_subheader">
        <div class="container d-flex align-items-center subheader-custom mt-3 justify-content-between flex-wrap flex-sm-nowrap">
            <div class="d-flex align-items-center flex-wrap mr-1">
                <div class="d-flex align-items-baseline flex-wrap mr-10">
                    <i class="flaticon-home-1 header-icon"></i>
                    <h3 class="text-white icon-title">
                        @MasterResources.HomeLbl
                    </h3>
                </div>
            </div>
        </div>
        <div class="topbar-item language-item">
            <div class="dropdown">
                <!--begin::Toggle-->
                <div class="topbar-item" data-toggle="dropdown" aria-expanded="false">
                    <div class="btn btn-icon btn-clean btn-dropdown btn-lg mr-1">
                        <img class="h-20px w-20px rounded-sm" src="@LanguageImgSrc" alt="">
                    </div>
                </div>
                <!--end::Toggle-->
                <!--begin::Dropdown-->
                <div class="dropdown-menu p-0 m-0 dropdown-menu-anim-up dropdown-menu-sm dropdown-menu-right">
                    <!--begin::Nav-->
                    <ul class="navi navi-hover py-4">
                        <!--begin::Item-->
                        @if (Request.Cookies["Language"].Value == "en")
                        {
                        <li class="navi-item">
                            <a href="@Url.Action("ChangeLanguage", "Master", new {language = "ar"})"  class="navi-link">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="~/Content/images/158-egypt.svg" alt="">
                                </span>
                                <span class="navi-text language-item-label">@MasterResources.ArabicLbl</span>
                            </a>
                        </li>
                        }
                        else
                        {
                        <!--end::Item-->
                        <!--begin::Item-->

                        <li class="navi-item active">
                            <a href="@Url.Action("ChangeLanguage", "Master", new {language = "en"})"  class="navi-link">
                                <span class="symbol symbol-20 mr-3">
                                    <img src="~/Content/images/226-united-states.svg" alt="">
                                </span>
                                <span class="navi-text language-item-label">@MasterResources.EnglishLbl</span>
                            </a>
                        </li>
                        }
                        <!--end::Item-->

                    </ul>
                    <!--end::Nav-->
                </div>
                <!--end::Dropdown-->
            </div>
        </div>
