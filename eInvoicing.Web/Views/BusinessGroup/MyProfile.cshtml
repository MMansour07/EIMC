@model eInvoicing.Web.Models.EditViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <!--begin::Card-->
            <div class="card card-custom gutter-b example example-compact">
                <div class="card-header bg-light">
                    <h3 class="card-title" style="float: left !important">
                        My Profile  
                    </h3>
                    <div class="card-toolbar">
                    </div>
                </div>
   
                    <div class="card-body">
                        <!--begin::Group-->
                        <div class="form-group row">
                            <div class="col-lg-12 col-xl-9">
                                @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control form-control-solid form-control-lg", @placeholder = "First Name" , @disabled = "true" })
                               

                            </div>
                        </div>
                        <!--end::Group-->
                        <!--begin::Group-->
                        <div class="form-group row">
                            <div class="col-lg-12 col-xl-9">
                                @Html.TextBoxFor(model => model.LastName, new { @class = "form-control form-control-solid form-control-lg", @placeholder = "Last Name", @disabled = "true" })

                            </div>
                        </div>
                        <!--end::Group-->
                        <!--begin::Group-->
                        <div class="form-group row">
                            <div class="col-lg-12 col-xl-9">
                                <div class="input-group input-group-solid input-group-lg">
                                    @Html.TextBoxFor(model => model.Email, new { @class = "form-control form-control-solid form-control-lg", @placeholder = "Email", @type = "email" , @disabled = "true" })
                                </div>
                            </div>
                        </div>
                        <!--end::Group-->
                        <!--begin::Group-->
                        <div class="form-group row">
                            <div class="col-lg-12 col-xl-9">
                                @Html.TextBoxFor(model => model.Title, new { @class = "form-control form-control-solid form-control-lg", @placeholder = "Title" , @disabled = "true" })
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-lg-12 col-xl-9">
                                <div class="input-group input-group-solid input-group-lg">
                                    @Html.TextBoxFor(model => model.PhoneNumber, new { @class = "form-control form-control-solid form-control-lg", @placeholder = "Phone Number" , @disabled = "true" })

                                </div>
                            </div>
                        </div>
                        
                            <div class="form-group row">
                                <div class="col-lg-12 col-xl-9">
                                    @Html.ListBoxFor(model => model.Roles, (List<SelectListItem>)ViewBag.RolesList, new { @class = "form-control select2", @id = "kt_select2_3", @name = "param", @multiple = "multiple", @placeholder = "Roles" , @disabled = "true"})
                                </div>
                            </div>
                        
                        <!--end::Group-->
                    </div>
                <!--end::Form-->
            </div>
            <!--end::Card-->
        </div>
    </div>
</div>





