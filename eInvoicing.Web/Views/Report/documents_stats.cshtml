@model IEnumerable<eInvoicing.DTO.DocumentDTO>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="subheader py-2 py-lg-6  subheader-transparent" id="kt_subheader">
    <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
        <div class="d-flex align-items-center flex-wrap mr-1">
            <div class="d-flex align-items-baseline flex-wrap mr-5">
                <!--begin::Breadcrumb-->
                <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                    <li class="breadcrumb-item">
                        <a href="@Url.Action("Index", "Master")" class="text-white">
                            Home
                        </a>
                    </li>
                    <li class="breadcrumb-item">
                        <a href="javascript:window.location.reload();" class="text-white">
                            Documents Statistics
                        </a>
                    </li>
                </ul>
                <!--end::Breadcrumb-->
            </div>
        </div>
    </div>
</div>
<div class="d-flex flex-column-fluid">
    <div class="container-fluid mt-65">
        <div class="form-group row mb-4">
            <div class="col-lg-3 col-md-6 col-sm-12">
                <div class="input-group date">
                    <input type="text" class="form-control" id="fromDate" readonly="readonly" placeholder="From Date">
                    <div class="input-group-append">
                        <span class="input-group-text">
                            <i class="la la-calendar"></i>
                        </span>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 col-sm-12">
                <div class="input-group date">
                    <input type="text" class="form-control" id="toDate" readonly="readonly" placeholder="To Date">
                    <div class="input-group-append">
                        <span class="input-group-text">
                            <i class="la la-calendar"></i>
                        </span>
                    </div>
                </div>
            </div>
            <a href="#" id="_find" class="btn btn-sm btn-primary font-weight-bold">
                Find
                <i class="flaticon-search ml-1"></i>
            </a>
        </div>
        <hr />
        <div class="card card-custom  gutter-b" id="SubmittedDocumentsStats_crd">
            @*<div class="card-header bg-light">
                    <div class="card-title">
                        <span class="card-icon"><i class="flaticon2-layers text-primary"></i></span>
                        <h3 class="card-label">Submitted Documents Analytics & Stats</h3>
                    </div>
                </div>*@
            <div class="card-body">
                <!--begin: Datatable-->
                <table class="table table-striped table-bordered table-hover display nowrap" id="kt_datatable10">
                    <thead>
                        <tr>
                            <th>Issued On</th>
                            <th>Total</th>
                            <th>Valid</th>
                            <th>Invalid</th>
                            <th>Failed</th>
                            <th>Submitted</th>
                            <th>Cancelled</th>
                            <th>Rejected</th>
                            <th>Submitted On</th>
                            <th>Submitted By</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>Total</th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                            <th></th>
                        </tr>
                    </tfoot>
                </table>
                <!--end: Datatable-->
            </div>
        </div>
    </div>
</div>
<script src="~/Content/assets/plugins/custom/datatables/datatables.bundle.js?v=7.0.6"></script>
<script src="~/Content/assets/js/pages/crud/datatables/search-options/column-search.js?v=7.0.6"></script>
<script src="~/Content/assets/plugins/custom/fullcalendar/fullcalendar.bundle.js?v=7.0.6"></script>
<script src="~/Content/assets/js/pages/crud/forms/widgets/bootstrap-datepicker.js"></script>
